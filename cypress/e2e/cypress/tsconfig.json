{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "types": ["cypress", "node"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["cypress/**/*", "src/**/*"],
    "exclude": ["node_modules"]
  }